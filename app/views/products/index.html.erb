<div class = "prodlist1">
  <div class = "borderlist2">
    <h3>Trading House</h3>
    <table class = "table">
      <tr>
        <th>Name</th>
        <th>Price</th>
        <th>Owner</th>
        <th>Status</th>
        <th>Action</th>
      </tr>
    <% @products.each do |products| %>
      <tr>
      <td><%= products.name %></td>
      <td><%= products.price %></td>
      <td><%= products.user.firstname %> <%= products.user.lastname%></td>
      <td><%= products.status %></td>
      <td><%= link_to "See Product Profile", product_path(products.id)%></td>
    </tr>
  <% end %>
  </table>
  <%= will_paginate @products %>

</div>
</div>
